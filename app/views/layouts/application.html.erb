	<div id="user_header"> -->
		<%#= link_to "Sign_up", new_user_path %>
	<!-- </div> -->

  
  <%# if current_user %>
      <!-- Logged in as <%#= current_user.email %>. -->
      <%#= link_to "Log Out", logout_path %>
  <%# else %>
      <%#= link_to "Sign Up", signup_path %><br>
      <%#= link_to "Log In", login_path %>
  <%# end %>
   
	
  <%# flash.each do |name,msg| %>
		<%#= content_tag :div, msg, id: "flash_#{name}" %>
	<%# end %>

<%= yield %>

</body>
</html>
